@model KisiselWebProjesi_MVC.Models.Siniflar.ikonlar
@{
    ViewBag.Title = "YeniIkon";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 class="mt-2">Yeni İkon Ekle</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x=>x.ikon)
    @Html.TextBoxFor(x=>x.ikon, new {@class="form-control"})
    <br />
    @Html.LabelFor(x=>x.link)
    @Html.TextBoxFor(x=>x.ikon, new {@class="form-control"})
    <br />
    <button class="btn btn-sm btn-success">İkon Ekle</button>
</form>

